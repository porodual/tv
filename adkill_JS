var c=document.getElementsByTagName("body");var ele_body=c[0];for(var i=0;i<ele_body.childElementCount;i++){var ele=ele_body.children[i];if(window.getComputedStyle(ele).position=="fixed"){if(i==0){var i=0;if(ele.tagName.toLowerCase=="div"){i=1}if(ele.tagName.toLowerCase=="nav"){i=1}if(ele.tagName.toLowerCase=="header"){i=1}if(i==1){ele.style.display="none"}}if(i!=0){if(parseInt(window.getComputedStyle(ele).zIndex)>=5){ele.style.display="none"}}}};
