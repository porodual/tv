var c=document.getElementsByTagName("body");var ele_body=c[0];function traversal(g){var b=g.length;var f=null;for(var e=0;e<b;e++){var a=false;f=g[e];console.log(f.nodeName);if(f.nodeName=="IMG"&&f.parentNode.nodeName=="A"){f.style.display="none";a=true}if(f.nodeName=="img"&&f.parentNode.nodeName=="a"){f.style.display="none";a=true}if(a==true){findfinalparent(f)}if(f.children){traversal(f.children)}}}function findfinalparent(a){var d=a;var b=0;for(;;){if(b>30){return}if((d.parentNode.nodeName=="body")||(d.parentNode.nodeName=="BODY")){d.style.display="none";return}else{d=d.parentNode}}}for(var i=0;i<ele_body.childElementCount;i++){var ele=ele_body.children[i];if(window.getComputedStyle(ele).position=="fixed"){traversal(ele.childNodes)}};
